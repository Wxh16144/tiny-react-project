{
  "name": "tiny-react-project",
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint \"src/**/*.{ts,js,tsx,jsx}\"",
    "eslint:fix": "eslint \"src/**/*.{ts,js,tsx,jsx}\" --fix",
    "stylelint": "stylelint \"src/**/*.{css,less}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,less}\" --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "npm run eslint:fix",
      "git add"
    ],
    "src/**/*.{css,less}": [
      "npm run stylelint:fix",
      "git add"
    ]
  },
  "dependencies": {
    "@wuxh/utils": "0.0.2",
    "antd": "^4.17.3",
    "mobx": "^6.5.0",
    "mobx-react-lite": "^3.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.24.0",
    "react-location": "^3.3.0",
    "swiper": "^8.0.6"
  },
  "devDependencies": {
    "@types/node": "^17.0.1",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^1.0.7",
    "@wuxh/commitlint-config": "0.0.2",
    "@wuxh/eslint-config-react": "*",
    "@wuxh/stylelint-config-basic": "0.0.3",
    "colors": "^1.4.0",
    "commitlint": "^15.0.0",
    "eslint": "^8.4.1",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^12.1.2",
    "postcss-less": "^5.0.0",
    "stylelint": "^14.1.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.1",
    "vite-plugin-style-import": "^1.4.0",
    "vite-plugin-windicss": "^1.6.1",
    "windicss": "^3.4.2"
  }
}
