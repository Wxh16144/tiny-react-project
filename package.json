{
  "name": "tiny-react-project",
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint \"src/**/*.{ts,js,tsx,jsx}\"",
    "eslint:fix": "eslint \"src/**/*.{ts,js,tsx,jsx}\" --fix",
    "stylelint": "stylelint \"src/**/*.{css,less}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,less}\" --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "npm run eslint:fix",
      "git add"
    ],
    "src/**/*.{css,less}": [
      "npm run stylelint:fix",
      "git add"
    ]
  },
  "packageManager": "pnpm@8.6.2",
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@wuxh/utils": "^1.1.0",
    "ahooks": "^3.7.8",
    "antd": "^5.12.2",
    "mobx": "^6.12.0",
    "mobx-react-lite": "^4.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "swiper": "^11.0.5",
    "vite-plugin-pages": "^0.32.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "@wuxh/commitlint-config": "0.0.2",
    "@wuxh/eslint-config-react": "*",
    "@wuxh/stylelint-config-basic": "0.0.3",
    "colors": "^1.4.0",
    "commitlint": "^18.4.3",
    "eslint": "^8.55.0",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "lint-staged": "^15.2.0",
    "postcss-less": "^6.0.0",
    "stylelint": "^16.0.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-plugin-windicss": "^1.9.2",
    "windicss": "^3.5.6"
  }
}
