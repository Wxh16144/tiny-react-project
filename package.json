{
  "name": "tiny-react-project",
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint \"src/**/*.{ts,js,tsx,jsx}\"",
    "eslint:fix": "eslint \"src/**/*.{ts,js,tsx,jsx}\" --fix",
    "stylelint": "stylelint \"src/**/*.{css,less}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,less}\" --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "npm run eslint:fix",
      "git add"
    ],
    "src/**/*.{css,less}": [
      "npm run stylelint:fix",
      "git add"
    ]
  },
  "packageManager": "pnpm@7.3.0",
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@emotion/babel-plugin": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@tanstack/react-location": "^3.7.4",
    "@wuxh/utils": "^0.0.3",
    "ahooks": "^3.7.0",
    "antd": "^5.0.1",
    "mobx": "^6.7.0",
    "mobx-react-lite": "^3.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.2.1",
    "swiper": "^8.4.5"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^2.2.0",
    "@wuxh/commitlint-config": "0.0.2",
    "@wuxh/eslint-config-react": "*",
    "@wuxh/stylelint-config-basic": "0.0.3",
    "colors": "^1.4.0",
    "commitlint": "^17.3.0",
    "eslint": "^8.28.0",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.2",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "postcss-less": "^6.0.0",
    "stylelint": "^14.15.0",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-plugin-style-import": "^1.4.1",
    "vite-plugin-windicss": "^1.8.8",
    "windicss": "^3.5.6"
  }
}
